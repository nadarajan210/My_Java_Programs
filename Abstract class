abstract class writer{
	public abstract void show();
	public abstract void display();
 }

class pen extends writer{ 
	public void show() {
		System.out.println("Pen Wrints in blue color");
	}
	public void display() {
		System.out.println("Pencil Writs in grey color");
	}
}

public class abstractclass {

	public static void main(String[] args) {
		pen obj = new pen();
		obj.display();
		obj.show();

	}

}

/*
Pencil writs in grey color
Pen Wrints in blue color 
*/
